{
  "npm.binPath": "yarn",
  "coc.preferences.extensionUpdateCheck": "never",
  "coc.preferences.formatOnType": false,
  "coc.preferences.formatOnSaveFiletypes": ["markdown", "vimwiki"],
  "coc.preferences.enableFloatHighlight": false,
  "coc.preferences.colorSupport": false,
  "coc.source.file.ignoreHidden": false,
  "coc.source.file.triggerCharacters": ["/", ".", ".."],
  "markdownlint.config": {
    "default": true,
    "line_length": false,
    "no-duplicate-heading": {
      "siblings_only": true
    },
    "no-trailing-punctuation": false
  },
  "prettier.requireConfig": false,
  "prettier.onlyUseLocalVersion": false,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "snippets.extends": { "vimwiki": ["markdown", "vimwiki"] },
  "snippets.userSnippetsDirectory": "~/.config/nvim/snips/UltiSnips",
  // "smartf.wordJump": false,
  // "smartf.jumpOnTrigger": false,
  "diagnostic.refreshOnInsertMode": true,
  "coc.preferences.bracketEnterImprove": false,
  "yank.priority": 70,
  "yank.highlight.enable": false,
  "explorer.position": "floating",
  "explorer.file.showHiddenFiles": true,
  "cSpell.languageSettings": [
    { "languageId": "*", "dictionaries": ["typescript", "node"] }
  ],
  "cSpellExt.enableDictionaries": ["spanish"],
  "cSpell.language": "en,es",
  "coc-actions.hideCursor": false
  //  "diagnostic-languageserver.enable": false,
